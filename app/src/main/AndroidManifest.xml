<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.musiccloud">
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".Room.BasicApp"
        android:allowBackup="true"
        android:usesCleartextTraffic="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MusicCloud">
        <activity
            android:name=".Room.RecyclerView.HotList.HotListActivity"
            android:exported="false" />
        <activity
            android:name=".Room.RecyclerView.NewList.NewListActivity"
            android:exported="false" />
        <activity
            android:name=".Room.RecyclerView.VipList.VipListActivity"
            android:exported="false" />
        <activity
            android:name=".MvActivity.FirstMvActivity"
            android:exported="false" />
        <activity
            android:name=".MvActivity.SecondMvActivity"
            android:exported="false" />
        <activity
            android:name=".MvActivity.ThirdMvActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>

</manifest>